#include "main.h"

/**
 *_islower -checks if letter is lowercase
 *
 *Return: 1 if c is lower case and 0 otherwise
 */

int _islower(int c)
{

if (c <= 'z' && c >= 'a')
{
return (1);
}

else
{
return (0);
}
_putchar('\n');

}

/**
 *main-Check code
 *Return: 0(Success)
 */

int main(void)
{
int c;
_islower(c);
return (0);
}
